<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="256.0" minWidth="600.0" prefHeight="256.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.farmas.controller.game.questions.types.SAController">
   <children>
      <Label fx:id="questionLabel" alignment="CENTER" styleClass="titleQuestion" text="Label" textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="themeLabel" styleClass="subTitle" text="Label">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Label>
      <JFXTextField fx:id="shortAnswerField" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </JFXTextField>
   </children>
   <stylesheets>
      <URL value="@../sheetstyles/app.css" />
      <URL value="@../sheetstyles/question.css" />
   </stylesheets>
</VBox>
