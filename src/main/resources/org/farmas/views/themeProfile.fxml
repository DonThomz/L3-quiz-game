<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="160.0" maxWidth="160.0" minHeight="160.0" minWidth="160.0" prefHeight="160.0" prefWidth="160.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.farmas.controller.theme.ThemeProfile">
    <children>
        <JFXButton fx:id="themeButton" buttonType="RAISED" prefHeight="122.0" prefWidth="115.0"
                   style="-fx-background-color: fafafa;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <graphic>
                <AnchorPane>
                    <children>
                        <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <AnchorPane>
                                    <children>
                                        <HBox alignment="CENTER" AnchorPane.bottomAnchor="0.0"
                                              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                                              AnchorPane.topAnchor="0.0">
                                            <children>
                                                <ImageView fx:id="image" pickOnBounds="true" preserveRatio="true"/>
                                            </children>
                                        </HBox>
                                    </children>
                                </AnchorPane>
                                <Label fx:id="title" ellipsisString="" styleClass="titleTheme" text="Label"
                                       textAlignment="CENTER" wrapText="true" VBox.vgrow="ALWAYS">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="20.0"/>
                                    </VBox.margin>
                                    <padding>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </padding>
                                </Label>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </graphic>
        </JFXButton>
    </children>
</AnchorPane>
